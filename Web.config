<configuration>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.5.1"/>
    <httpRuntime targetFramework="4.5"/>
    <!--<sessionState timeout="60"></sessionState>-->
  </system.web>
  <!-- estabelece conexao com banco de dados através da string de conexão na classe de mapeamento-->
  <appSettings>
    <add key="strConexao" value="Database=espetinhogoiania;Data Source=localhost;User Id=root;Password=12345678; pooling=false"/>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None"/>
  </appSettings>
</configuration>